package com.tavant.collection;

import com.tavant.collection.models.Employee;

public class Main {
	public static void main(String[] args) {
		Employee[] employees = new Employee[5];
		
		for(int i = 0 ; i < employees.length ; i ++) {
			employees[i] = new Employee();
			employees[i].setEmployeeFirstName("Virendra" + i );
			
		}
		
		Employee[] temp = new Employee[employees.length + 5];
		System.arraycopy(employees, 0, temp, 0 , employees.length);
		
		for(Employee q : temp) {
			System.out.println(q);
		}
	}
}